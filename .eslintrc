{
  "env": {
    "browser": true,
    "es2020": true,
    "jest": true
  },
  "extends": ["react-app", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "printWidth": 100,
        "trailingComma": "es5",
        "semi": true,
        "jsxSingleQuote": true,
        "singleQuote": false,
        "useTabs": true,
        "endOfLine": "auto"
      }
    ]
  }
}
